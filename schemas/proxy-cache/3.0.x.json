{
  "fields": [
    {
      "config": {
        "type": "record",
        "fields": [
          {
            "response_code": {
              "elements": {
                "type": "integer",
                "between": [
                  100,
                  900
                ]
              },
              "len_min": 1,
              "type": "array",
              "required": true,
              "default": [
                200,
                301,
                404
              ],
              "examples": [
                [
                  200
                ]
              ]
            }
          },
          {
            "request_method": {
              "required": true,
              "elements": {
                "type": "string",
                "one_of": [
                  "HEAD",
                  "GET",
                  "POST",
                  "PATCH",
                  "PUT"
                ]
              },
              "type": "array",
              "default": [
                "GET",
                "HEAD"
              ],
              "examples": [
                [
                  "GET",
                  "HEAD"
                ]
              ]
            }
          },
          {
            "content_type": {
              "required": true,
              "elements": {
                "type": "string"
              },
              "type": "array",
              "default": [
                "text/plain",
                "application/json"
              ],
              "examples": [
                [
                  "text/plain",
                  "application/json"
                ]
              ]
            }
          },
          {
            "cache_ttl": {
              "gt": 0,
              "type": "integer",
              "default": 300,
              "examples": [
                300
              ]
            }
          },
          {
            "strategy": {
              "required": true,
              "type": "string",
              "one_of": [
                "memory"
              ],
              "examples": [
                "memory"
              ]
            }
          },
          {
            "cache_control": {
              "required": true,
              "type": "boolean",
              "default": false
            }
          },
          {
            "storage_ttl": {
              "type": "integer"
            }
          },
          {
            "memory": {
              "type": "record",
              "fields": [
                {
                  "dictionary_name": {
                    "required": true,
                    "type": "string",
                    "default": "kong_db_cache"
                  }
                }
              ],
              "required": true
            }
          },
          {
            "vary_query_params": {
              "type": "array",
              "elements": {
                "type": "string"
              }
            }
          },
          {
            "vary_headers": {
              "type": "array",
              "elements": {
                "type": "string"
              }
            }
          }
        ],
        "required": true
      }
    }
  ]
}