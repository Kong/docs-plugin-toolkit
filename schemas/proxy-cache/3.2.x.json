{
  "fields": [
    {
      "protocols": {
        "elements": {
          "one_of": [
            "grpc",
            "grpcs",
            "http",
            "https",
            "tcp",
            "tls",
            "tls_passthrough",
            "udp",
            "ws",
            "wss"
          ],
          "type": "string"
        },
        "required": true,
        "type": "set",
        "default": [
          "grpc",
          "grpcs",
          "http",
          "https"
        ]
      }
    },
    {
      "config": {
        "fields": [
          {
            "response_code": {
              "required": true,
              "elements": {
                "between": [
                  100,
                  900
                ],
                "type": "integer"
              },
              "type": "array",
              "len_min": 1,
              "default": [
                200,
                301,
                404
              ],
              "examples": [
                [
                  200
                ]
              ]
            }
          },
          {
            "request_method": {
              "elements": {
                "one_of": [
                  "HEAD",
                  "GET",
                  "POST",
                  "PATCH",
                  "PUT"
                ],
                "type": "string"
              },
              "type": "array",
              "required": true,
              "default": [
                "GET",
                "HEAD"
              ],
              "examples": [
                [
                  "GET",
                  "HEAD"
                ]
              ]
            }
          },
          {
            "content_type": {
              "elements": {
                "type": "string"
              },
              "type": "array",
              "required": true,
              "default": [
                "text/plain",
                "application/json"
              ],
              "examples": [
                [
                  "text/plain",
                  "application/json"
                ]
              ]
            }
          },
          {
            "cache_ttl": {
              "type": "integer",
              "gt": 0,
              "default": 300,
              "examples": [
                300
              ]
            }
          },
          {
            "strategy": {
              "type": "string",
              "required": true,
              "one_of": [
                "memory"
              ],
              "examples": [
                "memory"
              ]
            }
          },
          {
            "cache_control": {
              "type": "boolean",
              "required": true,
              "default": false
            }
          },
          {
            "storage_ttl": {
              "type": "integer"
            }
          },
          {
            "memory": {
              "fields": [
                {
                  "dictionary_name": {
                    "type": "string",
                    "required": true,
                    "default": "kong_db_cache"
                  }
                }
              ],
              "type": "record",
              "required": true
            }
          },
          {
            "vary_query_params": {
              "elements": {
                "type": "string"
              },
              "type": "array"
            }
          },
          {
            "vary_headers": {
              "elements": {
                "type": "string"
              },
              "type": "array"
            }
          }
        ],
        "type": "record",
        "required": true
      }
    }
  ]
}