                {
                  "json": {
                    "default": [

                    ],
                    "required": true,
                    "type": "array",
                    "elements": {
                      "type": "string",
                      "match": "^[^:]+:.*$"
                    },
                    "description": "List of `property:value` pairs. If and only if the property is not present, add a new property with the given value to the JSON body. Ignored if the property is already present."
