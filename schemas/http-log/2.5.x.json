{
  "fields": [
    {
      "protocols": {
        "required": true,
        "default": [
          "grpc",
          "grpcs",
          "http",
          "https"
        ],
        "type": "set",
        "elements": {
          "type": "string",
          "one_of": [
            "grpc",
            "grpcs",
            "http",
            "https",
            "tcp",
            "tls",
            "udp"
          ]
        }
      }
    },
    {
      "config": {
        "required": true,
        "type": "record",
        "fields": [
          {
            "http_endpoint": {
              "required": true,
              "type": "string",
              "examples": [
                "http://mockbin.org/bin/:id"
              ]
            }
          },
          {
            "method": {
              "default": "POST",
              "one_of": [
                "POST",
                "PUT",
                "PATCH"
              ],
              "type": "string",
              "examples": [
                "POST"
              ]
            }
          },
          {
            "content_type": {
              "default": "application/json",
              "one_of": [
                "application/json"
              ],
              "type": "string"
            }
          },
          {
            "timeout": {
              "type": "number",
              "default": 10000,
              "examples": [
                1000
              ]
            }
          },
          {
            "keepalive": {
              "type": "number",
              "default": 60000,
              "examples": [
                1000
              ]
            }
          },
          {
            "retry_count": {
              "type": "integer",
              "default": 10,
              "examples": [
                15
              ]
            }
          },
          {
            "queue_size": {
              "type": "integer",
              "default": 1
            }
          },
          {
            "flush_timeout": {
              "type": "number",
              "default": 2,
              "examples": [
                2
              ]
            }
          },
          {
            "headers": {
              "keys": {
                "type": "string",
                "match_none": [
                  {
                    "pattern": "^[Hh][Oo][Ss][Tt]$",
                    "err": "cannot contain 'Host' header"
                  },
                  {
                    "pattern": "^[Cc][Oo][Nn][Tt][Ee][Nn][Tt]%-[Ll][Ee][nn][Gg][Tt][Hh]$",
                    "err": "cannot contain 'Content-Length' header"
                  },
                  {
                    "pattern": "^[Cc][Oo][Nn][Tt][Ee][Nn][Tt]%-[Tt][Yy][Pp][Ee]$",
                    "err": "cannot contain 'Content-Type' header"
                  }
                ]
              },
              "type": "map",
              "values": {
                "type": "array",
                "elements": {
                  "type": "string"
                }
              }
            }
          },
          {
            "custom_fields_by_lua": {
              "keys": {
                "type": "string",
                "len_min": 1
              },
              "type": "map",
              "values": {
                "type": "string",
                "len_min": 1
              }
            }
          }
        ]
      }
    }
  ]
}