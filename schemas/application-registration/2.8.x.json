{
  "fields": [
    {
      "consumer": {
        "eq": null,
        "reference": "consumers",
        "type": "foreign"
      }
    },
    {
      "service": {
        "type": "foreign",
        "ne": null,
        "reference": "services",
        "on_delete": "cascade"
      }
    },
    {
      "route": {
        "eq": null,
        "reference": "routes",
        "type": "foreign"
      }
    },
    {
      "protocols": {
        "required": true,
        "elements": {
          "one_of": [
            "grpc",
            "grpcs",
            "http",
            "https"
          ],
          "type": "string"
        },
        "default": [
          "grpc",
          "grpcs",
          "http",
          "https"
        ],
        "type": "set"
      }
    },
    {
      "config": {
        "required": true,
        "fields": [
          {
            "display_name": {
              "required": true,
              "unique": true,
              "type": "string",
              "examples": [
                "<my_service_display_name>"
              ]
            }
          },
          {
            "description": {
              "unique": true,
              "type": "string",
              "examples": [
                "<my_service_description>"
              ]
            }
          },
          {
            "auto_approve": {
              "required": true,
              "default": false,
              "type": "boolean",
              "examples": [
                false
              ]
            }
          },
          {
            "show_issuer": {
              "required": true,
              "default": false,
              "type": "boolean",
              "examples": [
                false
              ]
            }
          }
        ],
        "type": "record"
      }
    }
  ]
}