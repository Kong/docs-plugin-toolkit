{
  "fields": [
    {
      "consumer": {
        "eq": null,
        "type": "foreign",
        "reference": "consumers"
      }
    },
    {
      "protocols": {
        "elements": {
          "one_of": [
            "grpc",
            "grpcs",
            "http",
            "https"
          ],
          "type": "string"
        },
        "required": true,
        "type": "set",
        "default": [
          "grpc",
          "grpcs",
          "http",
          "https"
        ]
      }
    },
    {
      "config": {
        "fields": [
          {
            "assertion_consumer_path": {
              "match_none": [
                {
                  "err": "must not have empty segments",
                  "pattern": "//"
                }
              ],
              "required": true,
              "starts_with": "/",
              "type": "string",
              "examples": [
                "/acs-uri"
              ]
            }
          },
          {
            "idp_sso_url": {
              "type": "string",
              "required": true,
              "examples": [
                "http://example.org/sso-uri"
              ]
            }
          },
          {
            "idp_certificate": {
              "type": "string",
              "encrypted": true,
              "required": false,
              "referenceable": true,
              "examples": [
                "-----BEGIN CERTIFICATE-----\n<certificate contents>\n-----END CERTIFICATE-----\n"
              ]
            }
          },
          {
            "response_encryption_key": {
              "type": "string",
              "encrypted": true,
              "required": false,
              "referenceable": true
            }
          },
          {
            "request_signing_key": {
              "type": "string",
              "encrypted": true,
              "required": false,
              "referenceable": true
            }
          },
          {
            "request_signing_certificate": {
              "type": "string",
              "encrypted": true,
              "required": false,
              "referenceable": true
            }
          },
          {
            "request_signature_algorithm": {
              "one_of": [
                "SHA256",
                "SHA384",
                "SHA512"
              ],
              "required": false,
              "type": "string",
              "default": "SHA256"
            }
          },
          {
            "request_digest_algorithm": {
              "one_of": [
                "SHA256",
                "SHA1"
              ],
              "required": false,
              "type": "string",
              "default": "SHA256"
            }
          },
          {
            "response_signature_algorithm": {
              "one_of": [
                "SHA256",
                "SHA384",
                "SHA512"
              ],
              "required": false,
              "type": "string",
              "default": "SHA256"
            }
          },
          {
            "response_digest_algorithm": {
              "one_of": [
                "SHA256",
                "SHA1"
              ],
              "required": false,
              "type": "string",
              "default": "SHA256"
            }
          },
          {
            "issuer": {
              "type": "string",
              "required": true,
              "examples": [
                "AzureAD_Identity_ID"
              ]
            }
          },
          {
            "nameid_format": {
              "one_of": [
                "Unspecified",
                "EmailAddress",
                "Persistent",
                "Transient"
              ],
              "required": false,
              "type": "string",
              "default": "EmailAddress"
            }
          },
          {
            "validate_assertion_signature": {
              "required": false,
              "type": "boolean",
              "default": true,
              "examples": [
                true
              ]
            }
          },
          {
            "anonymous": {
              "type": "string",
              "required": false,
              "examples": [
                "anonymous"
              ]
            }
          },
          {
            "session_secret": {
              "encrypted": true,
              "referenceable": true,
              "match": "^[0-9a-zA-Z/_+]+$",
              "type": "string",
              "len_min": 32,
              "len_max": 32,
              "required": true,
              "examples": [
                "XXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX"
              ]
            }
          },
          {
            "session_audience": {
              "required": false,
              "type": "string",
              "default": "default"
