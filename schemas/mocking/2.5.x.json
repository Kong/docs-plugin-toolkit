{
  "fields": [
    {
      "config": {
        "required": true,
        "type": "record",
        "fields": [
          {
            "api_specification_filename": {
              "type": "string",
              "required": false
            }
          },
          {
            "api_specification": {
              "type": "string",
              "required": false,
              "examples": [
                "openapi: 3.0.1\ninfo:\n  title: OpenAPI 3.0 API\n  description: This is a simple API\n  contact:\n    email: example@example.com\n  version: 1.0.0\n"
              ]
            }
          },
          {
            "random_delay": {
              "type": "boolean",
              "default": false,
              "examples": [
                true
              ]
            }
          },
          {
            "max_delay_time": {
              "type": "number",
              "default": 1,
              "examples": [
                1
              ]
            }
          },
          {
            "min_delay_time": {
              "type": "number",
              "default": 0.001,
              "examples": [
                0.001
              ]
            }
          }
        ]
      }
    }
  ]
}