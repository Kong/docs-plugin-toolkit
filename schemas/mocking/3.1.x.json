{
  "fields": [
    {
      "protocols": {
        "type": "set",
        "elements": {
          "type": "string",
          "one_of": [
            "grpc",
            "grpcs",
            "http",
            "https"
          ]
        },
        "default": [
          "grpc",
          "grpcs",
          "http",
          "https"
        ],
        "required": true
      }
    },
    {
      "config": {
        "type": "record",
        "fields": [
          {
            "api_specification_filename": {
              "required": false,
              "type": "string"
            }
          },
          {
            "api_specification": {
              "type": "string",
              "required": false,
              "examples": [
                "openapi: 3.0.1\ninfo:\n  title: OpenAPI 3.0 API\n  description: This is a simple API\n  contact:\n    email: example@example.com\n  version: 1.0.0\n"
              ]
            }
          },
          {
            "random_delay": {
              "default": false,
              "type": "boolean",
              "examples": [
                true
              ]
            }
          },
          {
            "max_delay_time": {
              "default": 1,
              "type": "number",
              "examples": [
                1
              ]
            }
          },
          {
            "min_delay_time": {
              "default": 0.001,
              "type": "number",
              "examples": [
                0.001
              ]
            }
          },
          {
            "random_examples": {
              "default": false,
              "type": "boolean",
              "examples": [
                true
              ]
            }
          },
          {
            "included_status_codes": {
              "type": "array",
              "elements": {
                "type": "integer"
              }
            }
          },
          {
            "random_status_code": {
              "default": false,
              "required": true,
              "type": "boolean"
            }
          }
        ],
        "required": true
      }
    }
  ]
}