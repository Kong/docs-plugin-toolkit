{
  "fields": [
    {
      "consumer": {
        "eq": null,
        "type": "foreign",
        "reference": "consumers"
      }
    },
    {
      "protocols": {
        "elements": {
          "one_of": [
            "grpc",
            "grpcs",
            "http",
            "https"
          ],
          "type": "string"
        },
        "required": true,
        "type": "set",
        "default": [
          "grpc",
          "grpcs",
          "http",
          "https"
        ]
      }
    },
    {
      "config": {
        "fields": [
          {
            "anonymous": {
              "type": "string"
            }
          },
          {
            "consumer_by": {
              "type": "array",
              "required": false,
              "elements": {
                "one_of": [
                  "username",
                  "custom_id"
                ],
                "type": "string"
              },
              "default": [
                "username",
                "custom_id"
              ]
            }
          },
          {
            "ca_certificates": {
              "required": true,
              "type": "array",
              "elements": {
                "uuid": true,
                "type": "string"
              },
              "examples": [
                [
                  "fdac360e-7b19-4ade-a553-6dd22937c82f"
                ]
              ]
            }
          },
          {
            "cache_ttl": {
              "required": true,
              "type": "number",
              "default": 60
            }
          },
          {
            "skip_consumer_lookup": {
              "required": true,
              "type": "boolean",
              "default": false
            }
          },
          {
            "allow_partial_chain": {
              "required": true,
              "type": "boolean",
              "default": false
            }
          },
          {
            "authenticated_group_by": {
              "one_of": [
                "CN",
                "DN"
              ],
              "required": false,
              "type": "string",
              "default": "CN"
            }
          },
          {
            "revocation_check_mode": {
              "one_of": [
                "SKIP",
                "IGNORE_CA_ERROR",
                "STRICT"
              ],
              "required": false,
              "type": "string",
              "default": "IGNORE_CA_ERROR"
            }
          },
          {
            "http_timeout": {
              "default": 30000,
              "type": "number"
            }
          },
          {
            "cert_cache_ttl": {
              "default": 60000,
              "type": "number"
            }
          },
          {
            "send_ca_dn": {
              "default": false,
              "type": "boolean"
            }
          },
          {
            "http_proxy_host": {
              "type": "string",
              "examples": [
                "example"
              ]
            }
          },
          {
            "http_proxy_port": {
              "between": [
                0,
                65535
              ],
              "type": "integer",
              "examples": [
                80
              ]
            }
          },
          {
            "https_proxy_host": {
              "type": "string"
            }
          },
          {
            "https_proxy_port": {
              "between": [
                0,
                65535
              ],
              "type": "integer"
            }
          }
        ],
        "type": "record",
        "entity_checks": [
          {
            "mutually_required": [
              "http_proxy_host",
              "http_proxy_port"
            ]
          },
          {
            "mutually_required": [
              "https_proxy_host",
              "https_proxy_port"
            ]
          }
        ],
        "required": true
      }
    }
  ]
}