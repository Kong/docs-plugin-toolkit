{
  "fields": [
    {
      "protocols": {
        "required": true,
        "elements": {
          "one_of": [
            "grpc",
            "grpcs",
            "http",
            "https"
          ],
          "type": "string"
        },
        "default": [
          "grpc",
          "grpcs",
          "http",
          "https"
        ],
        "type": "set"
      }
    },
    {
      "config": {
        "required": true,
        "fields": [
          {
            "second": {
              "gt": 0,
              "type": "number"
            }
          },
          {
            "minute": {
              "gt": 0,
              "type": "number"
            }
          },
          {
            "hour": {
              "gt": 0,
              "type": "number"
            }
          },
          {
            "day": {
              "gt": 0,
              "type": "number"
            }
          },
          {
            "month": {
              "gt": 0,
              "type": "number"
            }
          },
          {
            "year": {
              "gt": 0,
              "type": "number"
            }
          },
          {
            "limit_by": {
              "one_of": [
                "consumer",
                "credential",
                "ip",
                "service",
                "header",
                "path"
              ],
              "default": "consumer",
              "type": "string"
            }
          },
          {
            "header_name": {
              "type": "string"
            }
          },
          {
            "path": {
              "match_none": [
                {
                  "err": "must not have empty segments",
                  "pattern": "//"
                }
              ],
              "starts_with": "/",
              "type": "string"
            }
          },
          {
            "policy": {
              "one_of": [
                "local",
                "cluster",
                "redis"
              ],
              "len_min": 0,
              "default": "cluster",
              "type": "string"
            }
          },
          {
            "fault_tolerant": {
              "required": true,
              "default": true,
              "type": "boolean"
            }
          },
          {
            "redis_host": {
              "type": "string"
            }
          },
          {
            "redis_port": {
              "between": [
                0,
                65535
              ],
              "default": 6379,
              "type": "integer"
            }
          },
          {
            "redis_password": {
              "referenceable": true,
              "len_min": 0,
              "type": "string"
            }
          },
          {
            "redis_username": {
              "referenceable": true,
              "type": "string"
            }
          },
          {
            "redis_ssl": {
              "required": true,
              "default": false,
              "type": "boolean"
            }
          },
          {
            "redis_ssl_verify": {
              "required": true,
              "default": false,
              "type": "boolean"
            }
          },
          {
            "redis_server_name": {
              "type": "string"
            }
          },
          {
            "redis_timeout": {
              "default": 2000,
              "type": "number"
            }
          },
          {
            "redis_database": {
              "default": 0,
              "type": "integer"
            }
          },
          {
            "hide_client_headers": {
              "required": true,
              "default": false,
              "type": "boolean"
            }
          }
        ],
        "type": "record"
      }
    }
  ]
}