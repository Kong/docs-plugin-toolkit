{
  "fields": [
    {
      "protocols": {
        "elements": {
          "one_of": [
            "grpc",
            "grpcs",
            "http",
            "https"
          ],
          "type": "string"
        },
        "type": "set",
        "default": [
          "grpc",
          "grpcs",
          "http",
          "https"
        ],
        "required": true
      }
    },
    {
      "config": {
        "type": "record",
        "required": true,
        "fields": [
          {
            "second": {
              "gt": 0,
              "type": "number",
              "examples": [
                5
              ]
            }
          },
          {
            "minute": {
              "gt": 0,
              "type": "number"
            }
          },
          {
            "hour": {
              "gt": 0,
              "type": "number",
              "examples": [
                10000
              ]
            }
          },
          {
            "day": {
              "gt": 0,
              "type": "number"
            }
          },
          {
            "month": {
              "gt": 0,
              "type": "number"
            }
          },
          {
            "year": {
              "gt": 0,
              "type": "number"
            }
          },
          {
            "limit_by": {
              "type": "string",
              "one_of": [
                "consumer",
                "credential",
                "ip",
                "service",
                "header",
                "path"
              ],
              "default": "consumer"
            }
          },
          {
            "header_name": {
              "type": "string"
            }
          },
          {
            "path": {
              "starts_with": "/",
              "type": "string",
              "match_none": [
                {
                  "pattern": "//",
                  "err": "must not have empty segments"
                }
              ]
            }
          },
          {
            "policy": {
              "len_min": 0,
              "type": "string",
              "one_of": [
                "local",
                "cluster",
                "redis"
              ],
              "default": "cluster",
              "examples": [
                "local"
              ]
            }
          },
          {
            "fault_tolerant": {
              "type": "boolean",
              "default": true,
              "required": true
            }
          },
          {
            "redis_host": {
              "type": "string"
            }
          },
          {
            "redis_port": {
              "between": [
                0,
                65535
              ],
              "type": "integer",
              "default": 6379
            }
          },
          {
            "redis_password": {
              "len_min": 0,
              "type": "string"
            }
          },
          {
            "redis_timeout": {
              "default": 2000,
              "type": "number"
            }
          },
          {
            "redis_database": {
              "default": 0,
              "type": "integer"
            }
          },
          {
            "hide_client_headers": {
              "type": "boolean",
              "default": false,
              "required": true
            }
          }
        ]
      }
    }
  ]
}