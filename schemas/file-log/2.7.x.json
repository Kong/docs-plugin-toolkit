{
  "fields": [
    {
      "protocols": {
        "default": [
          "grpc",
          "grpcs",
          "http",
          "https"
        ],
        "elements": {
          "type": "string",
          "one_of": [
            "grpc",
            "grpcs",
            "http",
            "https",
            "tcp",
            "tls",
            "tls_passthrough",
            "udp"
          ]
        },
        "required": true,
        "type": "set"
      }
    },
    {
      "config": {
        "fields": [
          {
            "path": {
              "err": "not a valid filename",
              "type": "string",
              "required": true,
              "match": "^[^*&%%\\`]+$",
              "examples": [
                "/tmp/file.log"
              ]
            }
          },
          {
            "reopen": {
              "type": "boolean",
              "required": true,
              "default": false
            }
          },
          {
            "custom_fields_by_lua": {
              "keys": {
                "type": "string",
                "len_min": 1
              },
              "type": "map",
              "values": {
                "type": "string",
                "len_min": 1
              }
            }
          }
        ],
        "type": "record",
        "required": true
      }
    }
  ]
}