{
  "fields": [
    {
      "protocols": {
        "elements": {
          "one_of": [
            "grpc",
            "grpcs",
            "http",
            "https",
            "tcp",
            "tls",
            "udp"
          ],
          "type": "string"
        },
        "type": "set",
        "default": [
          "grpc",
          "grpcs",
          "http",
          "https"
        ],
        "required": true
      }
    },
    {
      "config": {
        "type": "record",
        "required": true,
        "fields": [
          {
            "host": {
              "required": true,
              "type": "string",
              "examples": [
                "127.0.0.1"
              ],
              "description": "The IP address or host name to send data to."
            }
          },
          {
            "port": {
              "between": [
                0,
                65535
              ],
              "type": "integer",
              "required": true,
              "examples": [
                9999
              ],
              "description": "The port to send data to on the upstream server."
            }
          },
          {
            "timeout": {
              "default": 10000,
              "type": "number",
              "description": "An optional timeout in milliseconds when sending data to the upstream server."
            }
          },
          {
            "keepalive": {
              "default": 60000,
              "type": "number",
              "description": "An optional value in milliseconds that defines how long an idle connection lives before being closed."
            }
          },
          {
            "tls": {
              "type": "boolean",
              "default": false,
              "required": true,
              "description": "Indicates whether to perform a TLS handshake against the remote server."
            }
          },
          {
            "tls_sni": {
              "type": "string",
              "description": "An optional string that defines the SNI (Server Name Indication) hostname to send in the TLS handshake."
            }
          },
          {
            "custom_fields_by_lua": {
              "keys": {
                "len_min": 1,
                "type": "string"
              },
              "type": "map",
              "values": {
                "len_min": 1,
                "type": "string"
              },
              "description": "A list of key-value pairs, where the key is the name of a log field and\nthe value is a chunk of Lua code, whose return value sets or replaces\nthe log field value."
            }
          }
        ]
      }
    }
  ]
}