{
  "fields": [
    {
      "protocols": {
        "elements": {
          "one_of": [
            "grpc",
            "grpcs",
            "http",
            "https",
            "tcp",
            "tls",
            "udp"
          ],
          "type": "string"
        },
        "type": "set",
        "default": [
          "grpc",
          "grpcs",
          "http",
          "https"
        ],
        "required": true
      }
    },
    {
      "config": {
        "type": "record",
        "required": true,
        "fields": [
          {
            "host": {
              "required": true,
              "type": "string",
              "examples": [
                "127.0.0.1"
              ]
            }
          },
          {
            "port": {
              "between": [
                0,
                65535
              ],
              "type": "integer",
              "required": true,
              "examples": [
                9999
              ]
            }
          },
          {
            "timeout": {
              "default": 10000,
              "type": "number"
            }
          },
          {
            "keepalive": {
              "default": 60000,
              "type": "number"
            }
          },
          {
            "tls": {
              "type": "boolean",
              "default": false,
              "required": true
            }
          },
          {
            "tls_sni": {
              "type": "string"
            }
          },
          {
            "custom_fields_by_lua": {
              "keys": {
                "len_min": 1,
                "type": "string"
              },
              "type": "map",
              "values": {
                "len_min": 1,
                "type": "string"
              }
            }
          }
        ]
      }
    }
  ]
}