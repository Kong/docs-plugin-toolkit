{
  "fields": [
    {
      "consumer": {
        "type": "foreign",
        "reference": "consumers",
        "eq": null
      }
    },
    {
      "protocols": {
        "type": "set",
        "elements": {
          "type": "string",
          "one_of": [
            "grpc",
            "grpcs",
            "http",
            "https"
          ]
        },
        "default": [
          "grpc",
          "grpcs",
          "http",
          "https"
        ],
        "required": true
      }
    },
    {
      "config": {
        "type": "record",
        "fields": [
          {
            "graphql_server_path": {
              "required": true,
              "match_none": [
                {
                  "pattern": "//",
                  "err": "must not have empty segments"
                }
              ],
              "starts_with": "/",
              "type": "string",
              "default": "/graphql",
              "examples": [
                "/graphql"
              ],
              "description": "The path to the GraphQL server."
            }
          }
        ],
        "required": true
      }
    }
  ]
}