{
  "fields": [
    {
      "protocols": {
        "required": true,
        "elements": {
          "one_of": [
            "grpc",
            "grpcs",
            "http",
            "https"
          ],
          "type": "string"
        },
        "default": [
          "grpc",
          "grpcs",
          "http",
          "https"
        ],
        "type": "set"
      }
    },
    {
      "config": {
        "required": true,
        "fields": [
          {
            "bootstrap_servers": {
              "elements": {
                "fields": [
                  {
                    "host": {
                      "required": true,
                      "type": "string"
                    }
                  },
                  {
                    "port": {
                      "required": true,
                      "between": [
                        0,
                        65535
                      ],
                      "type": "integer"
                    }
                  }
                ],
                "type": "record"
              },
              "type": "set"
            }
          },
          {
            "topic": {
              "required": true,
              "type": "string",
              "examples": [
                "TOPIC"
              ]
            }
          },
          {
            "timeout": {
              "default": 10000,
              "type": "integer"
            }
          },
          {
            "keepalive": {
              "default": 60000,
              "type": "integer"
            }
          },
          {
            "keepalive_enabled": {
              "default": false,
              "type": "boolean"
            }
          },
          {
            "authentication": {
              "required": true,
              "fields": [
                {
                  "strategy": {
                    "required": false,
                    "one_of": [
                      "sasl"
                    ],
                    "type": "string"
                  }
                },
                {
                  "mechanism": {
                    "required": false,
                    "one_of": [
                      "PLAIN",
                      "SCRAM-SHA-256",
                      "SCRAM-SHA-512"
                    ],
                    "type": "string"
                  }
                },
                {
                  "tokenauth": {
                    "required": false,
                    "type": "boolean"
                  }
                },
                {
                  "user": {
                    "required": false,
                    "referenceable": true,
                    "encrypted": true,
                    "type": "string"
                  }
                },
                {
                  "password": {
                    "required": false,
                    "referenceable": true,
                    "encrypted": true,
                    "type": "string"
                  }
                }
              ],
              "type": "record"
            }
          },
          {
            "security": {
              "required": true,
              "fields": [
                {
                  "certificate_id": {
                    "uuid": true,
                    "required": false,
                    "type": "string"
                  }
                },
                {
                  "ssl": {
                    "required": false,
                    "type": "boolean"
                  }
                }
              ],
              "type": "record"
            }
          },
          {
            "cluster_name": {
              "required": false,
              "auto": true,
              "type": "string"
            }
          },
          {
            "producer_request_acks": {
              "one_of": [
                -1,
                0,
                1
              ],
              "default": 1,
              "type": "integer"
            }
          },
          {
            "producer_request_timeout": {
              "default": 2000,
              "type": "integer"
            }
          },
          {
            "producer_request_limits_messages_per_request": {
              "default": 200,
              "type": "integer"
            }
          },
          {
            "producer_request_limits_bytes_per_request": {
              "default": 1048576,
              "type": "integer"
            }
          },
          {
            "producer_request_retries_max_attempts": {
              "default": 10,
              "type": "integer"
            }
          },
          {
            "producer_request_retries_backoff_timeout": {
              "default": 100,
              "type": "integer"
            }
          },
          {
            "producer_async": {
              "default": true,
              "type": "boolean"
            }
          },
          {
            "producer_async_flush_timeout": {
              "default": 1000,
              "type": "integer"
            }
          },
          {
            "producer_async_buffering_limits_messages_in_memory": {
              "default": 50000,
              "type": "integer"
            }
          }
        ],
        "entity_checks": [
          {
            "custom_entity_check": {
              "field_sources": [
                "authentication"
              ]
            }
          }
        ],
        "type": "record"
      }
    }
  ]
}