{
  "fields": [
    {
      "protocols": {
        "type": "set",
        "required": true,
        "elements": {
          "type": "string",
          "one_of": [
            "grpc",
            "grpcs",
            "http",
            "https",
            "tcp",
            "tls",
            "udp"
          ]
        },
        "default": [
          "grpc",
          "grpcs",
          "http",
          "https"
        ]
      }
    },
    {
      "config": {
        "type": "record",
        "required": true,
        "fields": [
          {
            "host": {
              "type": "string",
              "default": "logs-01.loggly.com"
            }
          },
          {
            "port": {
              "type": "integer",
              "between": [
                0,
                65535
              ],
              "default": 514
            }
          },
          {
            "key": {
              "type": "string",
              "required": true
            }
          },
          {
            "tags": {
              "type": "set",
              "elements": {
                "type": "string"
              },
              "default": [
                "kong"
              ]
            }
          },
          {
            "log_level": {
              "type": "string",
              "one_of": [
                "debug",
                "info",
                "notice",
                "warning",
                "err",
                "crit",
                "alert",
                "emerg"
              ],
              "default": "info"
            }
          },
          {
            "successful_severity": {
              "type": "string",
              "one_of": [
                "debug",
                "info",
                "notice",
                "warning",
                "err",
                "crit",
                "alert",
                "emerg"
              ],
              "default": "info"
            }
          },
          {
            "client_errors_severity": {
              "type": "string",
              "one_of": [
                "debug",
                "info",
                "notice",
                "warning",
                "err",
                "crit",
                "alert",
                "emerg"
              ],
              "default": "info"
            }
          },
          {
            "server_errors_severity": {
              "type": "string",
              "one_of": [
                "debug",
                "info",
                "notice",
                "warning",
                "err",
                "crit",
                "alert",
                "emerg"
              ],
              "default": "info"
            }
          },
          {
            "timeout": {
              "type": "number",
              "default": 10000
            }
          }
        ]
      }
    }
  ]
}