{
  "fields": [
    {
      "config": {
        "fields": [
          {
            "timeout": {
              "type": "number",
              "default": 600000
            }
          },
          {
            "keepalive": {
              "type": "number",
              "default": 60000
            }
          },
          {
            "https": {
              "type": "boolean",
              "default": true
            }
          },
          {
            "https_verify": {
              "type": "boolean",
              "default": false
            }
          },
          {
            "apikey": {
              "type": "string",
              "encrypted": true,
              "examples": [
                "<AZURE_APIKEY>"
              ]
            }
          },
          {
            "clientid": {
              "type": "string",
              "encrypted": true
            }
          },
          {
            "appname": {
              "type": "string",
              "required": true,
              "examples": [
                "<AZURE_APPNAME>"
              ]
            }
          },
          {
            "hostdomain": {
              "type": "string",
              "required": true,
              "default": "azurewebsites.net",
              "examples": [
                "azurewebsites.net"
              ]
            }
          },
          {
            "routeprefix": {
              "type": "string",
              "default": "api"
            }
          },
          {
            "functionname": {
              "type": "string",
              "required": true,
              "examples": [
                "<AZURE_FUNCTIONNAME>"
              ]
            }
          }
        ],
        "type": "record",
        "required": true
      }
    }
  ]
}