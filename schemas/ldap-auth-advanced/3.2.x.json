{
  "fields": [
    {
      "protocols": {
        "elements": {
          "one_of": [
            "grpc",
            "grpcs",
            "http",
            "https",
            "ws",
            "wss"
          ],
          "type": "string"
        },
        "required": true,
        "type": "set",
        "default": [
          "grpc",
          "grpcs",
          "http",
          "https",
          "ws",
          "wss"
        ]
      }
    },
    {
      "consumer": {
        "eq": null,
        "type": "foreign",
        "reference": "consumers"
      }
    },
    {
      "config": {
        "fields": [
          {
            "ldap_host": {
              "type": "string",
              "required": true,
              "examples": [
                "ldap.example.com"
              ]
            }
          },
          {
            "ldap_password": {
              "type": "string",
              "encrypted": true,
              "referenceable": true
            }
          },
          {
            "ldap_port": {
              "type": "number",
              "default": 389,
              "examples": [
                389
              ]
            }
          },
          {
            "bind_dn": {
              "type": "string",
              "referenceable": true
            }
          },
          {
            "ldaps": {
              "type": "boolean",
              "required": true,
              "default": false
            }
          },
          {
            "start_tls": {
              "type": "boolean",
              "required": true,
              "default": false,
              "examples": [
                true
              ]
            }
          },
          {
            "verify_ldap_host": {
              "type": "boolean",
              "required": true,
              "default": false,
              "examples": [
                false
              ]
            }
          },
          {
            "base_dn": {
              "type": "string",
              "required": true,
              "examples": [
                "dc=example,dc=com"
              ]
            }
          },
          {
            "attribute": {
              "type": "string",
              "required": true,
              "examples": [
                "cn"
              ]
            }
          },
          {
            "cache_ttl": {
              "type": "number",
              "required": true,
              "default": 60,
              "examples": [
                60
              ]
            }
          },
          {
            "hide_credentials": {
              "type": "boolean",
              "default": false
            }
          },
          {
            "timeout": {
              "type": "number",
              "default": 10000
            }
          },
          {
            "keepalive": {
              "type": "number",
              "default": 60000
            }
          },
          {
            "anonymous": {
              "type": "string",
              "len_min": 0,
              "default": ""
            }
          },
          {
            "header_type": {
              "type": "string",
              "default": "ldap",
              "examples": [
                "ldap"
              ]
            }
          },
          {
            "consumer_optional": {
              "type": "boolean",
              "required": false,
              "default": false
            }
          },
          {
            "consumer_by": {
              "type": "array",
              "required": false,
              "elements": {
                "one_of": [
                  "username",
                  "custom_id"
                ],
                "type": "string"
              },
              "default": [
                "username",
                "custom_id"
              ]
            }
          },
          {
            "group_base_dn": {
              "type": "string"
            }
          },
          {
            "group_name_attribute": {
              "type": "string"
            }
          },
          {
            "group_member_attribute": {
              "type": "string",
              "default": "memberOf"
            }
          },
          {
            "log_search_results": {
              "type": "boolean",
              "required": false,
              "default": false
            }
          },
          {
            "groups_required": {
              "type": "array",
              "required": false,
              "elements": {
                "type": "string"
              }
            }
          }
        ],
        "type": "record",
        "required": true
      }
    }
  ]
}