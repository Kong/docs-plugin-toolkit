                          "azure_client_secret": {
                            "type": "string",
                            "referenceable": true,
                            "description": "If azure_use_managed_identity is set to true, and you need to use a different user-assigned identity for this LLM instance, set the client secret.",
                            "required": false,
                            "encrypted": true
                          }
                        },
                        {
                          "azure_tenant_id": {
                            "required": false,
                            "type": "string",
