{
  "fields": [
    {
      "protocols": {
        "type": "set",
        "elements": {
          "type": "string",
          "one_of": [
            "grpc",
            "grpcs",
            "http",
            "https",
            "tcp",
            "tls",
            "tls_passthrough",
            "udp",
            "ws",
            "wss"
          ]
        },
        "default": [
          "grpc",
          "grpcs",
          "http",
          "https"
        ],
        "required": true
      }
    },
    {
      "config": {
        "type": "record",
        "fields": [
          {
            "host": {
              "required": true,
              "type": "string",
              "examples": [
                "127.0.0.1"
              ]
            }
          },
          {
            "port": {
              "required": true,
              "between": [
                0,
                65535
              ],
              "type": "integer",
              "examples": [
                9999
              ]
            }
          },
          {
            "timeout": {
              "type": "number",
              "default": 10000,
              "examples": [
                10000
              ]
            }
          },
          {
            "custom_fields_by_lua": {
              "type": "map",
              "keys": {
                "type": "string",
                "len_min": 1
              },
              "values": {
                "type": "string",
                "len_min": 1
              }
            }
          }
        ],
        "required": true
      }
    }
  ]
}